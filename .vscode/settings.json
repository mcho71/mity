{
    "[markdown]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "editor.formatOnSave": true,
    "cSpell.words": [
        "Descrete",
        "EOSIO",
        "InnoDB",
        "Sarsa",
        "bigint",
        "cleos",
        "githubactions",
        "mity",
        "nodeos",
        "pullreminders"
    ],
    "search.exclude": {
        "**/content": true
    },
    "pasteImage.basePath": "${currentFileDir}",
    "pasteImage.path": "${currentFileDir}/image"
}